
        <div class="page-wrapper">
            <!-- ============================================================== -->
            <!-- Bread crumb and right sidebar toggle -->
            <!-- ============================================================== -->
            <div class="page-breadcrumb bg-white">
                <div class="row align-items-center">
                    <div class="col-lg-3 col-md-4 col-sm-4 col-xs-12">
                        <h4 class="page-title">User Manager</h4>
                    </div>
                    <div class="col-lg-9 col-sm-8 col-md-8 col-xs-12">
                        <div class="d-md-flex">
                            <ol class="breadcrumb ms-auto">
                                <li><a href="/admin" class="fw-normal">Dashboard</a></li>
                            </ol>
                            
                        </div>
                    </div>
                </div>
                <!-- /.col-lg-12 -->
            </div>
            <div class="container-fluid">
                <!-- ============================================================== -->
                <!-- Start Page Content -->
                <!-- ============================================================== -->
                <div class="row">
                    <div class="col-sm-12">
                        <div class="white-box">
                            {{!-- <h3 class="box-title">Basic Table</h3>
                            <p class="text-muted">Add class <code>.table</code></p> --}}
                            <div class="table-responsive">
                                <table class="table text-nowrap">
                                    <thead>
                                        <tr>
                                            <th class="border-top-0">#</th>
                                            <th class="border-top-0">Name</th>
                                            <th class="border-top-0">Email</th>
                                            <th class="border-top-0">Status</th>
                                        </tr>
                                    </thead>
                                    <tbody id="myTable">
                                        {{#each users}}
                                        <tr>
                                            <td>{{inc @index}}</td>
                                            <td>{{this.firstname}} {{this.lastname}}</td>
                                            {{#if this.email}}
                                            <td>{{this.email}}</td>
                                            {{else}}
                                            {{#if this.googleId}}
                                            <td>Google Id : {{this.googleId}}</td>
                                            {{/if}}
                                            {{#if this.githubId}}
                                            <td>Github Id : {{this.githubId}}</td>
                                            {{/if}}
                                            {{#if this.linkedId}}
                                            <td>LinkedIn Id : {{this.linkedId}}</td>
                                            {{/if}}
                                            {{/if}}
                                            {{#if this.block}}
                                            <td>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<a href="/admin/unblock-user/{{this._id}}" class="fa fa-ban" style="color:darkgreen;" onclick="return confirm('comfirm to unblock this user {{this.firstname}} {{this.lastname}}')"></a></td>                          
                                            {{else}}
                                            <td>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<a href="/admin/block-user/{{this._id}}" class="fa fa-ban" style="color: crimson;" onclick="return confirm('comfirm to block this user {{this.firstname}} {{this.lastname}}')"></a></td>
                                            {{/if}}
                                        </tr>
                                       {{/each}}
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
 
            </div>
           
        </div>

    </div>
  