

<main class="browse-section">
<div class="container">
<div class="row">
<div class="col-lg-3 col-md-4">
<div class="account_dt_left">
<div class="job-center-dt">
    {{#if userProfile.status}}
    <img id="imgview1" src="/profile-photos/{{userProfile._id}}.jpg" alt="">
    {{else}}
    <img id="imgview2" src="/images/profile_dp.jpg" alt="">
    {{/if}}<div class="job-urs-dts">

<h4>{{userProfile.firstname}} {{userProfile.lastname}}</h4>
<span>{{userProfile.tagline}}</span>
<div class="avialable">{{userProfile.availability}}</div>
</div>
</div>
<div class="my_websites">
<ul>
{{#if userProfile.link1}}
<li><a href="{{users.link1}}" class="web_link"><i class="fas fa-globe"></i>{{userProfile.link1}}</a></li>
{{/if}}
{{#if userProfile.link2}}
<li><a href="{{users.link2}}" class="web_link"><i class="far fa-edit"></i>{{userProfile.link2}}</a></li>
{{/if}}
{{#if userProfile.link3}}
<li><a href="{{users.link3}}" class="web_link"><i class="far fa-edit"></i>{{userProfile.link3}}</a></li>
{{/if}}
</ul>
</div>


<div class="rlt_section">


<ul class="rlt_section2">
<li>
<div class="rtl_left2">
<h6>Experenice</h6>
</div>
<div class="rtl_right2"> 
<span>{{userProfile.experience}}</span>
</div>
</li>
<li>
<div class="rtl_left2">
<h6>DOB</h6>
</div>
<div class="rtl_right2">
<span>{{userProfile.birthdate}}</span>
</div>
</li>
<li>
<div class="rtl_left2">
<h6>Contact</h6>
</div>
<div class="rtl_right2">
<span>{{userProfile.mobile}}</span>
</div>
</li>
<li>
<div class="rtl_left2">
<h6>Location</h6>
</div>
<div class="rtl_right2">
<span>{{userProfile.location}}</span>
</div>
</li>
</ul>
</div>
<div class="social_section3 mb80">
<div class="social_leftt3">
<h6>Contact Social Account</h6>
</div>

<ul class="social_accounts">
<li><a href="#" class="social_links"><i class="fab fa-facebook-f f1"></i>http://facebook.com/johndoe</a></li>
<li><a href="#" class="social_links"><i class="fab fa-twitter t1"></i>http://twitter.com/johndoe</a></li>
<li><a href="#" class="social_links"><i class="fab fa-linkedin-in l1"></i>http://linkedin.com/johndoe</a></li>
<li><a href="#" class="social_links"><i class="fab fa-dribbble d1"></i>http://dribbble.com/johndoe</a></li>
<li><a href="#" class="social_links"><i class="fab fa-behance b1"></i>http://behance.net/johndoe</a></li>
<li><a href="#" class="social_links"><i class="fab fa-github g1"></i>http://github.com/johndoe</a></li>
</ul>
</div>
</div>
</div>
<div class="col-lg-9 col-md-8 mainpage">
<div class="account_heading">
<div class="account_hd_left">
<a href="/employer" style="color: black;"><h4><i class="fas fa-arrow-circle-left"></i>Back to home</h4></a>
</div>

</div>
<div class="account_tabs">
<ul class="nav nav-tabs">


</ul>
</div>
<div class="view_chart">
<div class="view_chart_header">
<h4>About</h4>
</div>
<div class="view_chart_body">
<p class="user_about_des">{{userProfile.discription}}</p>
</div>
</div>
<div class="view_chart">
<div class="view_chart_header">
<h4>Skills</h4>
</div>
<div class="view_chart_body">
<div class="job-skills">
{{#each userProfile.skills}}
<a class="more-skills">{{this}}</a>
{{/each}}
</div>
</div>
</div>
<div class="view_chart">
<div class="view_chart_header">
<h4>Language</h4>
</div>
<div class="view_chart_body">
<div class="job-skills">
{{#each userProfile.language}}
<a class="more-skills">{{this}}</a>
{{/each}}
</div>
</div>
</div>
</div>
</div>
</div>
</main>


